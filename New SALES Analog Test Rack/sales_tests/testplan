sub Setup_Power_Supplies (Status_Code, Message$)
global Status
global Pslimit

   if Message$ <> "" then  print tab(5);Message$
   Status = Status_Code
   cps
!  sps 9,5.00,4.00;optimize      !5v
!  sps 12,12.00,2.00;optimize    !12v
!  sps 11,12.00,2.00;optimize    !12v_quiet
!  sps 10,12.00,0.80;optimize    !12v_relay
!  sps 5,10.00,2.00;optimize     !-10v
!  sps 6,10.00,2.00;optimize     !-10v_quiet

   sps 10,12.00,1.80,wa100m;optimize    !12v_relay
   sps 11,12.00,2.00;optimize    !12v_quiet
   sps 12,12.00,2.00;optimize    !12v
   wait1000m
   sps 9,5.00,5.00,wa500m;optimize      !5v
   sps 6,10.00,2.00;optimize     !-10v_quiet
   sps 5,10.00,2.00;optimize     !-10v
!  sps 9,5.00,5.00,wa500m;optimize      !5v

   Pslimit = pslimit
   pass device
   if Pslimit then
      dps
      fail device
      I = 1
      for Pscount = 1 to 12
         if binand (Pslimit,I) then
            report "Power Supply Number "
            report Pscount
            report "In Current Limit"
         end if
         I = 2*I
      next Pscount
      report "________________________________________"
      report "Check for backwards"
      report "IC's or Capacitors."
      report "________________________________________"
   end if
subend

sub Cycle_Power_On_Board
global PowerMsg$
global Failed_In_Power_Supplies
   call Disconnect_Power_On_Board
   call Setup_Power_Supplies (Failed_In_Power_Supplies, PowerMsg$)
subend

sub Change_Power_For_Edge
global PowerMsg$
global Failed_In_Power_Supplies

   sps 10,10.00,1.80;optimize    !12v_relay
   wait 100m
   sps 11,10.00,2.00;optimize    !12v_quiet
   sps 12,10.00,2.00;optimize    !12v
   wait 800m                  ! Allow Regulated 8.3v to stabalize
   sps 9,4.50,4.00;optimize
   wait 300m
   sps 6,10.00,2.00;optimize     !-10v_quiet
   sps 5,10.00,2.00;optimize     !-10v
subend

sub Disconnect_Power_On_Board
   rps 9,V,A,Err
   if Err<>225 then
     sps 9,0.00,1.50!;optimize      !5v
     wait300m
     sps 6,0.00,1.50!;optimize     !-10v_quiet
     wait300m
     sps 5,0.00,4.50!;optimize     !-10v
     wait300m
     sps 10,0.00,1.50!,wa100m;optimize    !12v_relay
     wait300m
     sps 11,0.00,1.50!;optimize    !12v_quiet
     wait300m
     sps 12,0.00,1.50!;optimize    !12v
     wait300m
     dps
   end if
subend

