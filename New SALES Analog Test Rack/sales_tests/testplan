sub Record_SN
global Serial$, System$, Report_Printer$

 P$ ="C:/Agilent_ICT/boards/result_log/result_heron"
assign @Result to P$; write, exclusive, append
output @Result; Serial$, datetime$, System$ ;" Passed"
assign @Result to *

!rint "Report_Printer$ = ";Report_Printer$

!report is *
!report is "/dev/com/2"
report "-------------------------------"
report "       ICT Station     ";System$
report "        BOARD PASSED           "
report
report " SN: "; Serial$
report " Datetime: "; datetime$
report "-------------------------------"
report ""
report ""
report ""
report ""

subend
