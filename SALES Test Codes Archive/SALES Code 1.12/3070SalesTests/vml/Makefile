$(CMD):
	@if hp9000s700; \
        then \
          echo "\tcreate dummy '$@'."; echo "#error NOT BUILT ON UNIX." > $@ || exit $$?; \
        else \
          doit="$(VMC) $(VMC_FLAGS) -command $? -output $@"; echo "\t$$doit"; eval $(VMC_PATH) $$doit || eval $(VMC_PATH) $$doit || exit $$?; \
        fi

MAX_ENV_LENGTH=2048

chk_PATH: disp_ver alwaysDoThis
	@if hp9000s700; \
      then \
        true ; \
      else \
        eval $(VMC_PATH); test $$(echo "$$PATH" | wc -c) -lt "$(MAX_ENV_LENGTH)" || echo "WARNING: \$$PATH NOT less than $(MAX_ENV_LENGTH) characters--it will be truncated.\n" ; \
      fi

dsp_PATH: disp_ver alwaysDoThis
	@if hp9000s700; \
      then \
        true ; \
      else \
        eval $(VMC_PATH) ; \
        echo "PATH:" ; \
        IFS=';' ; \
        for i in $$PATH ; \
        do \
          echo $$i ; \
        done \
      fi

sanity: alwaysDoThis
	@true

lint: disp_ver

clean: cmd_clean

cmd_clean: alwaysDoThis
	@doit="$(RM) $(RM_FLAGS) $(CMD)"; echo "\t$$doit"; eval $$doit

