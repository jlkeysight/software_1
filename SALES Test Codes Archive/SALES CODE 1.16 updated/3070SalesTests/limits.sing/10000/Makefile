all: limits sanity

limits: $(TXT)
	@if hp9000s700; \
        then \
          doit="touch $@";                               echo "\t$$doit"; eval $$doit || exit $$?; \
        else \
          doit="$(CAT) $(CAT_FLAGS) $(TXT) > $(IN)";     echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="$(RM) $(RM_FLAGS) $(OUT)";               echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="touch $(LIM_TO)";                        echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="$(LIM) $(LIM_FLAGS) $(IN)";              echo "\t$$doit"; eval $(LIM_PATH) $$doit || exit $$?; \
          doit="$(MV) $(MV_FLAGS) $(OUT) $@";            echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="$(RM) $(RM_FLAGS) $(IN) $(LIM_CLUTTER)"; echo "\t$$doit"; eval $$doit || exit $$?; \
        fi

MAX_ENV_LENGTH=2048

chk_PATH: disp_ver alwaysDoThis
	@if hp9000s700; \
      then \
        true ; \
      else \
        eval $(LIM_PATH); test $$(echo "$$PATH" | wc -c) -lt "$(MAX_ENV_LENGTH)" || echo "WARNING: \$$PATH NOT less than $(MAX_ENV_LENGTH) characters--it will be truncated.\n" ; \
      fi

dsp_PATH: disp_ver alwaysDoThis
	@if hp9000s700; \
      then \
        true ; \
      else \
        eval $(LIM_PATH) ; \
        echo "PATH:" ; \
        IFS=';' ; \
        for i in $$PATH ; \
        do \
          echo $$i ; \
        done \
      fi

sanity: alwaysDoThis
	@true

lint: disp_ver

clean: limits_clean

limits_clean: alwaysDoThis
	@doit="$(RM) $(RM_FLAGS) $(IN) $(LIM_CLUTTER) $(OUT) limits"; echo "\t$$doit"; eval $$doit

