	@if hp9000s700; \
        then \
          doit="touch $@"; echo "\t$$doit"; eval $$doit; \
        else \
          if test "$$(cat $(T) | wc -l)" -eq 0; \
          then \
            doit="touch $@"; echo "\t$$doit"; eval $$doit; \
