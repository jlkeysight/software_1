IN=text_file
OUT=limit_file
P01=$(PROJECT_ROOT)/u/src
P02=$(PROJECT_ROOT)/bin/src
P03=$(PROJECT_ROOT)/pvi/src/jade
P04=$(PROJECT_ROOT)/ascan/src
P05=$(PROJECT_ROOT)/dgn/src/ag_ictdgn
P06=$(PROJECT_ROOT)/dgn/src/3070Tests
P07=$(PROJECT_ROOT)/dgn/src/i5000Tests
P08=$(PROJECT_ROOT)/dgn/src/3070SalesTests
P09=$(PROJECT_ROOT)/dgn/src/i5000SalesTests
P10=$(PROJECT_ROOT)/qcomp/src
P11=$(PROJECT_ROOT)/u/src/ag3070u
P12=$(PROJECT_ROOT)/vth/src/ag3070vth
P13=$(PROJECT_ROOT)/vth/src/agi5000ftp
P14=$(PROJECT_ROOT)/pm/src/ag3070pm
P15=$(PROJECT_ROOT)/vmc/src/vmclib
P16=$(PROJECT_ROOT)/vmc/src/3070vmc
P17=$(PROJECT_ROOT)/vmc/src/raptorvmc
P18=$(PROJECT_ROOT)/licensing/src/cpp/dll
P_last=$(PROJECT_ROOT)/tools/installd_nt/files/third.pty/Microsoft
LIM_PATH=`hp9000s700 && \
  echo '' || \
  echo 'PATH="$(P01);$(P02);$(P03);$(P04);$(P05);$(P06);$(P07);$(P08);$(P09);$(P10);$(P11);$(P12);$(P13);$(P14);$(P15);$(P16);$(P17);$(P18);$(PATH);$(P_last)"'`
LIM=`hp9000s700 && \
  echo '../../../dgn$E' || \
  echo ''`
LIM_FLAGS=`hp9000s700 && \
  echo '-sales -dll_output -texttolimits' || \
  echo ''`
LIM_CLUTTER=`hp9000s700 && \
  echo 'limit_file testdesc glimits gtests testorder dgn.config' || \
  echo ''`
LIM_TO=`hp9000s700 && \
  echo 'testorder' || \
  echo ''`
CAT=`hp9000s700 && \
  echo 'cat' || \
  echo 'cat'`
CAT_FLAGS=`hp9000s700 && \
  echo '' || \
  echo ''`
MV=`hp9000s700 && \
  echo 'mv' || \
  echo 'mv'`
MV_FLAGS=`hp9000s700 && \
  echo '-f' || \
  echo '-f'`

all: limits sanity

limits: $(TXT)
	@if hp9000s700; \
        then \
          doit="$(CAT) $(CAT_FLAGS) $(TXT) > $(IN)";     echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="$(RM) $(RM_FLAGS) $(OUT)";               echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="touch $(LIM_TO)";                        echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="$(LIM) $(LIM_FLAGS) $(IN)";              echo "\t$$doit"; eval $(LIM_PATH) $$doit || exit $$?; \
          doit="$(MV) $(MV_FLAGS) $(OUT) $@";            echo "\t$$doit"; eval $$doit || exit $$?; \
          doit="$(RM) $(RM_FLAGS) $(IN) $(LIM_CLUTTER)"; echo "\t$$doit"; eval $$doit || exit $$?; \
        else \
          doit="touch $@";                               echo "\t$$doit"; eval $$doit || exit $$?; \
        fi

MAX_ENV_LENGTH=2048

chk_PATH: disp_ver alwaysDoThis
	@if hp9000s700; \
      then \
        true ; \
      else \
        eval $(LIM_PATH); test $$(echo "$$PATH" | wc -c) -lt "$(MAX_ENV_LENGTH)" || echo "WARNING: \$$PATH NOT less than $(MAX_ENV_LENGTH) characters--it will be truncated.\n" ; \
      fi

dsp_PATH: disp_ver alwaysDoThis
	@if hp9000s700; \
      then \
        true ; \
      else \
        eval $(LIM_PATH) ; \
        echo "PATH:" ; \
        IFS=';' ; \
        for i in $$PATH ; \
        do \
          echo $$i ; \
        done \
      fi

sanity: alwaysDoThis
	@true

lint: disp_ver

clean: limits_clean

limits_clean: alwaysDoThis
	@doit="$(RM) $(RM_FLAGS) $(IN) $(LIM_CLUTTER) $(OUT) limits"; echo "\t$$doit"; eval $$doit

